        <div>
            <i class="icon-bag font-red-sunglo margin-right-10"></i>
        </div>
        <div>
            {{#each selectedServices as |selectedService|}}
                <div class="animate-from-top">{{selectedService.name}}</div>
            {{/each}}
        </div>
    </div>
{{!-- {{/if}} --}}